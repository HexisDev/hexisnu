<p id="notice"><%= notice %></p>

<h1>Listing Seatmaps</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @seatmaps.each do |seatmap| %>
      <tr>
        <td><%= seatmap.name %></td>
        <td><%= link_to 'Show', seatmap %></td>
        <td><%= link_to 'Edit', edit_seatmap_path(seatmap) %></td>
        <td><%= link_to 'Destroy', seatmap, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Seatmap', new_seatmap_path %>
